{
  "name": "@rdfjs/data-model",
  "version": "1.3.4",
  "description": "A basic implementation of the RDFJS Data Model",
  "main": "index.js",
  "scripts": {
    "coverage": "codecov",
    "prepublish": "browserify index.js --standalone rdf --outfile dist/rdf-data-model.js",
    "test": "stricter-standard *.js bin/*.js lib/*.js test/*.js && c8 --reporter=lcov --reporter=text mocha"
  },
  "bin": {
    "rdfjs-data-model-test": "./bin/test.js"
  },
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/rdfjs-base/data-model.git"
  },
  "keywords": [
    "rdf",
    "rdfjs",
    "data-model"
  ],
  "author": "Thomas Bergwinkl <bergi@axolotlfarm.org> (https://www.bergnet.org/people/bergi/card#me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdfjs-base/data-model/issues"
  },
  "homepage": "https://github.com/rdfjs-base/data-model",
  "dependencies": {
    "@rdfjs/types": ">=1.0.1"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "c8": "^7.7.3",
    "codecov": "^3.8.2",
    "mocha": "^9.0.1",
    "stricter-standard": "^0.2.0"
  }
}
