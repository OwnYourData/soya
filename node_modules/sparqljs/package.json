{
  "name": "sparqljs",
  "version": "2.2.3",
  "description": "A parser for the SPARQL query language",
  "author": "Ruben Verborgh <ruben.verborgh@gmail.com>",
  "keywords": [
    "sparql",
    "rdf",
    "query",
    "parser"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RubenVerborgh/SPARQL.js.git"
  },
  "bugs": {
    "url": "https://github.com/RubenVerborgh/SPARQL.js/issues"
  },
  "main": "sparql.js",
  "bin": "bin/sparql-to-json",
  "files": [
    "sparql.js",
    "lib/*.js",
    "bin/*"
  ],
  "scripts": {
    "build": "./node_modules/.bin/jison lib/sparql.jison -p slr -o lib/SparqlParser.js",
    "test": "./node_modules/.bin/mocha"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "jison": "0.4.18",
    "mocha": "4.1.x",
    "chai": "4.1.x",
    "pre-commit": "~1.2.2"
  },
  "browser": {
    "_process": false,
    "fs": false,
    "path": false
  },
  "pre-commit": [
    "build",
    "test"
  ]
}
